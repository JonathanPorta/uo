!!!
html(lang="en")
	head
		meta(charset="utf-8")
		meta(name="viewport", content="width=device-width, initial-scale=1, maximum-scale=1")
		title ultimate opens
		link(rel="stylesheet", href="vendor/bootstrap/docs/assets/css/bootstrap.css")
		link(rel="stylesheet", href="vendor/bootstrap/docs/assets/css/bootstrap-responsive.css")
		link(rel="stylesheet", href="styles/components.css")
		link(rel="stylesheet", href="styles/pages.css")

		//- In Dependency order, or close enough.
		script(src="vendor/jquery/jquery.min.js")

		script(src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCpyNBlnrTdnn39KNDr-fwW35mYSmYQMpQ&sensor=false")

		script(src="vendor/underscore/underscore-min.js")
		script(src="vendor/superscore/lib/superscore.min.js")
		script(src="vendor/bootstrap/docs/assets/js/bootstrap.min.js")

		script(src="vendor/jefri/lib/jefri.min.js")
		script(src="vendor/jefri-stores/lib/jefri-stores.min.js")

		script(src="vendor/angular/angular.js")
		script(src="vendor/angular-ui/build/angular-ui.min.js")
		script(src="vendor/jefri-angular/build/dist/jefri-angular.min.js")

	body
		.app(ng:app="uo")
			//- This menu bar markup comes from Bootstrap's responsive header.
			.menu
				.navbar-inner
					.container
						//- The navDrop button expands and collapses the menu when responsively small.
						button.navDrop(type="button", data-toggle="collapse", data-target=".nav-collapse")
							span.icon-bar
							span.icon-bar
							span.icon-bar
						a.brand(href="#/") ultimate opens
						.nav-collapse
							ul.nav
								//- Add new top-level menu items here
								li
									a(href="#/map") Map
			.content
				ng:view

		#templates(style="display:none")
			include ../build/views/components.html
			include ../build/views/pages.html

		script(src="scripts/app.js")
		script(src="scripts/services.js")
		script(src="scripts/filters.js")
		script(src="scripts/directives.js")
		script(src="scripts/components.js")
		script(src="scripts/pages.js")
